<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.gil.gyrotouch">
    <application
        android:allowBackup="true"
        android:icon="@mipmap/gyrotouch_ic"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/gyrotouch_ic_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat.NoActionBar">
        <activity android:name=".Main">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".GyroTouch"></activity>
        <activity android:name=".Test"></activity>
        <activity android:name=".MeasureRepository"></activity>
        <activity android:name=".Valid"></activity>
        <activity android:name=".BeforeValid"></activity>
        <activity android:name=".Register"></activity>
        <activity android:name=".Info"></activity>
    </application>
</manifest>
